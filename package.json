{
  "name": "phylo-movies-frontend",
  "version": "0.64.0",
  "description": "PhyloMovies is a browser-based phylogenetic tree viewer with morphing animations for computational biologists.",
  "author": "Enes Berk Sakalli <enes.sakalli@univie.ac.at>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/enesBerkSakalli/phylo-movies.git"
  },
  "bugs": {
    "url": "https://github.com/enesBerkSakalli/phylo-movies/issues"
  },
  "homepage": "https://github.com/enesBerkSakalli/phylo-movies#readme",
  "keywords": [
    "phylogenetics",
    "visualization",
    "bioinformatics",
    "react",
    "WebGL",
    "deck.gl"
  ],
  "private": true,
  "scripts": {
    "preinstall": "bash scripts/check-submodule.sh",
    "postinstall": "bash scripts/check-submodule.sh",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "mocha --require @babel/register 'test/fileUpload.test.js' 'test/parser.test.js' 'test/integration.test.js' 'test/fileUploadTests.js' 'test/msaViewerIntegrationTests.js' 'test/msaViewerRefreshTest.js' 'test/tree-centering-verification.test.js' 'test/msa-workflow.test.js' 'test/scssModuleLoading.test.js' 'test/msa-automated-scrolling.test.js' 'test/timeline-construction.test.js' 'test/interpolation-cache-reset.test.js'",
    "test:watch": "mocha --require @babel/register --watch 'test/fileUpload.test.js' 'test/parser.test.js' 'test/integration.test.js' 'test/fileUploadTests.js' 'test/msaViewerIntegrationTests.js' 'test/msaViewerRefreshTest.js' 'test/msa-workflow.test.js' 'test/msa-automated-scrolling.test.js'",
    "test:msa": "mocha --require @babel/register 'test/msa-workflow.test.js' 'test/msa-automated-scrolling.test.js'",
    "test:msa-scrolling": "mocha --require @babel/register 'test/msa-automated-scrolling.test.js'",
    "demo:msa-scrolling": "node test/msa-scrolling-demo.js",
    "build:gh": "vite build --base=/phylo-movies/ && ./scripts/copy-examples.sh dist",
    "copy-examples": "./scripts/copy-examples.sh dist",
    "test:backend-required": "mocha --require @babel/register 'test/integration.test.js' 'test/msa-workflow.test.js'",
    "test:unit": "mocha --require @babel/register 'test/parser.test.js' 'test/fileUpload.test.js'",
    "test:scss": "mocha --require @babel/register 'test/scssModuleLoading.test.js'",
    "test:alignment2msa": "mocha --require @babel/register 'test/alignment2MSAViewCreation.test.js'",
    "test:tree-animation": "mocha --require @babel/register 'test/tree_animation_demo.test.js'",
    "test:backward-scrubbing": "mocha --require @babel/register 'test/backward-scrubbing-logic.test.js'",
    "test:interpolation-fix": "mocha --require @babel/register 'test/interpolation-engine-bug-fix.test.js'",
    "test:direction-detection": "mocha --require @babel/register 'test/scrubbing-direction-detection.test.js'",
    "test:backward-scrubbing-all": "npm run test:direction-detection && npm run test:backward-scrubbing && npm run test:interpolation-fix",
    "dev:electron": "cd electron-app && npm run dev"
  },
  "dependencies": {
    "@deck.gl/core": "9.2.5",
    "@deck.gl/extensions": "9.2.5",
    "@deck.gl/layers": "9.2.5",
    "@hookform/resolvers": "^5.2.2",
    "@nivo/bar": "^0.99.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vitejs/plugin-react": "^4.5.0",
    "bezier-js": "^6.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "colorjs.io": "^0.6.1",
    "d3": "^7.9.0",
    "d3-array": "^3.2.4",
    "localforage": "^1.10.0",
    "lucide-react": "^0.562.0",
    "memoize-one": "^6.0.0",
    "next-themes": "^0.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "^4.5.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.63.0",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^7.10.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.27.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.6.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "chai": "^4.5.0",
    "jsdom": "^26.1.0",
    "mocha": "^10.8.2",
    "proxyquire": "^2.1.3",
    "sass": "^1.89.1",
    "sinon": "^17.0.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^5.4.20",
    "vitest": "^3.2.4"
  }
}
